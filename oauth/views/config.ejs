<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <link rel="stylesheet" href="../css/styles.css">
    <meta charset="utf-8">
    <title>cmoorelabs</title>
  </head>
  <body>
    <div id="info">
      <p>discord bot</p>
      <% for(let i=0; i < data.length; i++) { %>
        <div class="card" onclick="updateCommand(getElementById('command_id'))">
          <p>Command:</p> <p id="command_name" contenteditable><%= data[i]['name'] %></p>
          <p id="command_id"><%= data[i]['id'] %></p>
          <% if(data[i]['type'] == 1) {%>
            <p>type: Chat %></p>
          <% } %>
          <% if(data[i]['type'] == 2) {%>
            <p>type: User %></p>
          <% } %>
          <% if(data[i]['type'] == 2) {%>
            <p>Type: Message %></p>
          <% } %>
          <p>Description: <%= data[i]['description'] %></p>
      </div>
      <% } %>

      <% if(guild == undefined)  { %>
      <p>Guilds</p>
      <div class="card">
        <p>Bot Not In Guild</p>
      </div>
      <% } %>

      <% if(guild != undefined)  { %>
      <p>Guilds</p>
      <div class="card">

        <p>Guild: <%= guild['name'] %></p>
      </div>
      <% } %>
  </body>
  <script type="application/javascript" src="../commands.js"></script>
</html>
